{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Simple Strategy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/simple/strategy.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "LOGURU_LEVEL": "INFO"
            }
        },
        {
            "name": "Run Put Spread Strategy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/putspread/strategy.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "LOGURU_LEVEL": "INFO"
            }
        },
        {
            "name": "Optimize Put Spread Strategy",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/putspread/optimize.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "LOGURU_LEVEL": "INFO"
            }
        }
    ]
}